doc_types () 
{ 
    : from backquotes in Markdown source, return Included name types;
    : related: pdmd;
    : date: 2023-09-23;
    report_notfile $1 && return 1;
    foreach ta $(cat $@ | identifyBackQuote) | awk '

    # name IS A ...
    
    $2 ~ /^is$/ && $3 ~ /^a$/ {

        # e.g. shell builtin
	
	printf "%s\t%s\n", $1, $4 ((NF>4)? " " $5:"")
	next
    }
    $2 ~ /^is$/ {

        # files, ...

	printf "%s\t%s\n", $1, $3
    }
    ' | sort
}
