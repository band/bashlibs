list_append () 
{ 
    : ~ listname new_member ...;
    : date: 2024-01-04;
    list_create $1 $($1) ${@:2}
}
list_create () 
{ 
    : ~ listname listmember ...;
    : date: 2024-01-04;
    : date: 2024-01-05;
    : date: 2024-01-06;
    : date: 2024-02-25;
    local message=": mfg by list_create; ";
    debug message: $message;
    eval "$1 () { $message \${@:-echo} ${@:2}; }";
    local lib=$(whsrc $1);
    debug lib: $lib;
    lib=${lib:-$(libfun)/cmdlinelib};
    debug lib: $lib;
    pause;
    lfs $lib $1;
    backup $lib
}
list_crunch () 
{ 
    : collapse a list into crunched format;
    : todo: fix list_next??, test on version_list;
    : date: 2024-01-23;
    : date: 2024-01-25;
    : date: 2024-09-02;
    : date: 2024-10-18;
    : date: 2025-05-01;
    : date: 2025-05-16;
    report_notfunction $1 && return 1;
    local nextlist=$($1 | list_next);
    debug nextlist: $nextlist;
    pause;
    eval "$1 () { : Crunched $1; \${@:-echo} $nextlist; }";
    declare -f $1
}
list_eg2 () 
{ 
    : add to a list;
    : date: 2025-03-17;
    list_list | tr ' ' '\n' > .list;
    ( cat .list;
    echo share_list_test \
               list_{example,eg2} ) | tpl | sort | list_crunch list_list
}
list_example () 
{ 
    : repair punch_list, remove _next;
    : date: 2025-03-17;
    set punch_list;
    $1 | tpl | grep -v _next | list_crunch $1
}
list_list () 
{ 
    : mfg by list_create;
    : date: 2025-05-01;
    ${@:-echo} list_append list_create list_crunch list_list list_remove list_review list_subtraction list_test list_eg2 list_example list_libheader list_next
}
list_remove () 
{ 
    : ~ listname remove_member ...;
    : todo: reject list duplicates;
    : date: 2024-01-04;
    : date: 2025-05-06;
    comment NOT yet Working, use manual editing;
    return;
    list_create $1 $(list_subtraction $($1) - ${@:2})
}
list_review () 
{ 
    : compare with List and args ...;
    : todo: eliminate TMP files?;
    : date: 2024-01-30;
    report_notfunction $1 && return 1;
    debug ENTRY $@;
    local fam=${1%_list};
    debug fam: $fam;
    $1 | tpl | sort -u > .lst.tmp;
    ( sfg ^$fam;
    ( echo ${@:2} | tpl ) ) | sort -u > .who.tmp;
    debug tmp, who: $(wc .{lst,who}.tmp);
    pause;
    comm .{lst,who}.tmp
}
list_subtraction () 
{ 
    : discourage use outside removal;
    : related: list_remove;
    : todo: reject list duplicates;
    : date: 2024-01-04;
    : date: 2024-06-19;
    : date: 2025-05-06;
    comment NOT yet Working, use manual editing;
    return;
    awk -f $INSTALL_HOME/lib/awk/list_subtraction.awk $@ 2> ~/tmp/$(myname).err;
    comment $HOME/tmp/$(myname).err
}
list_test () 
{ 
    : love those testcases.;
    : remove now gratuitiously avoids non list entries;
    : date: 2024-01-04;
    :;
    echo CREATE;
    list_create test_list a b c 3 2 1;
    echo;
    echo APPEND;
    list_append test_list how now cow;
    echo;
    echo REMOVE;
    list_remove test_list now joe 2 c;
    echo;
    echo LIST;
    list_list;
    echo;
    echo LIST_LIST;
    list_create list_list $(comm -12 <(functions listlib | sort) <(list_list))
}
