changeBak_list () 
{ 
    : the most recent BACKUP in any Directory in the tree;
    : list:;
    : date: 2023-10-23;
    : date: 2025-01-18;
    find . -type f | grep .bak/ | grep -v .bak/.bak/ | grep -v .dev/
}
chapter_list () 
{ 
    : the copy_specific source file chapters textfile list;
    : the table lists the un-suffexed file names to join;
    : with the Chapter Sequence, producing the included file;
    : the STDOUT produces the Markdown file list to format;
    : list:;
    : date: 2024-05-28;
    : date: 2024-06-28;
    : date: 2025-01-18;
    debug ENTRY $@;
    bashlib_src pushd;
    set -- $(docopy)/chapters.txt ./ChSequence.rdb;
    comment ? ./docCopy.rdb;
    newest $1 $2 || { 
        column file title comment < $2 | tee $1
    };
    local copy=$(docopy).rdb;
    debug $(llrt $@ $copy);
    pause;
    ls -alrt ${*:-.} $*;
    return;
    newest $@ || { 
        debug TOC $@;
        pause;
        cat $2 | rowhas copy $(docopy) | ncolumn copy > $copy;
        jointable ChSequence $3 $copy | md_chapter > $1
    };
    column file < $copy | headoff | sed 's/$/.md/'
}
